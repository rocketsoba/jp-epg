{
    "name": "rocketsoba/jp-epg",
    "description": "Scraper for 'https://timetable.yanbe.net/'",
    "type": "library",
    "authors": [
        {
            "name": "rockyakisoba",
            "email": "mmtrokuroku96@gmail.com"
        }
    ],
    "require": {
        "kub-at/php-simple-html-dom-parser": "^1.9",
        "rocketsoba/curl-builder": "^0.1",
        "rocketsoba/dom-parser-wrapper": "^0.2",
        "symfony/console": "^3.4",
        "symfony/monolog-bridge": "^3.4",
        "monolog/monolog": "^1.26"
    },
    "repositories": {
        "dom-parser-wrapper": {
            "type": "vcs",
            "url": "https://github.com/rocketsoba/dom-parser-wrapper",
            "no-api": true
        },
        "curl-builder": {
            "type": "vcs",
            "url": "https://github.com/rocketsoba/curl-builder",
            "no-api": true
        }
    },
    "autoload": {
        "psr-4": {
            "Rocketsoba\\EPG\\" : "src/"
        }
    }
